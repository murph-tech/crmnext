# CRM Deployment Environment Variables
# Copy this file to .env and fill in the values

# Database password (use a strong password!)
DB_PASSWORD=your_strong_database_password_here

# JWT Secret (generate a random string: openssl rand -base64 32)
JWT_SECRET=your_jwt_secret_key_here

# Frontend URL (your domain)
FRONTEND_URL=https://crm.yourdomain.com

# API URL (for frontend to call backend)
API_URL=https://api.crm.yourdomain.com
